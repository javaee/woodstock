{
    "name": "button",
    "type": "@JS_NAME@",
    "version": "@JS_VERSION@",
    "jmakiVersion": "1.8",
    "description": "Woodstock button widget.",
    "args": [
      {"alt": {
         "type": "STRING",
         "description": "Alternate text for image input.",
         "defaultValue": ""}},
      {"align": {
         "type": "STRING",
         "description": "Alignment of image input.",
         "defaultValue": ""}},
      {"className": {
         "type": "STRING",
         "description": "CSS selector.",
         "defaultValue": ""}},
      {"dir": {
         "type": "STRING",
         "description": "Specifies the directionality of text.",
         "defaultValue": ""}},
      {"disabled": {
         "type": "BOOLEAN",
         "description": "Disable element.",
         "defaultValue": false}},
      {"escape": {
         "type": "STRING",
         "description": "HTML escape value (default).",
         "defaultValue": ""}},
      {"id": {
         "type": "STRING",
         "description": "Uniquely identifies an element within a document.",
         "defaultValue": ""}},
      {"lang": {
         "type": "STRING",
         "description": "Specifies the language of attribute values and content.",
         "defaultValue": ""}},
      {"mini": {
         "type": "BOOLEAN",
         "description": "Set button as mini if true (N/A with src property).",
         "defaultValue": false}},
      {"onBlur": {
         "type": "STRING",
         "description": "Element lost focus.",
         "defaultValue": ""}},
      {"onClick": {
         "type": "STRING",
         "description": "Mouse button is clicked on element.",
         "defaultValue": ""}},
      {"onDblClick": {
         "type": "STRING",
         "description": "Mouse button is double-clicked on element.",
         "defaultValue": ""}},
      {"onFocus": {
         "type": "STRING",
         "description": "Element received focus.",
         "defaultValue": ""}},
      {"onKeyDown": {
         "type": "STRING",
         "description": "Key is pressed down over element.",
         "defaultValue": ""}},
      {"onKeyPress": {
         "type": "STRING",
         "description": "Key is pressed and released over element.",
         "defaultValue": ""}},
      {"onKeyUp": {
         "type": "STRING",
         "description": "Key is released over element.",
         "defaultValue": ""}},
      {"onMouseDown": {
         "type": "STRING",
         "description": "Mouse button is pressed over element.",
         "defaultValue": ""}},
      {"onMouseOut": {
         "type": "STRING",
         "description": "Mouse is moved away from element.",
         "defaultValue": ""}},
      {"onMouseOver": {
         "type": "STRING",
         "description": "Mouse is moved onto element.",
         "defaultValue": ""}},
      {"onMouseUp": {
         "type": "STRING",
         "description": "Mouse button is released over element.",
         "defaultValue": ""}},
      {"onMouseMove": {
         "type": "STRING",
         "description": "Mouse is moved while over element.",
         "defaultValue": ""}},
      {"prefix": {
         "type": "STRING",
         "description": "The application context path of image.",
         "defaultValue": ""}},
      {"primary": {
         "type": "BOOLEAN",
         "description": "Set button as primary if true (N/A with src property).",
         "defaultValue": false}},
      {"reset": {
         "type": "BOOLEAN",
         "description": "Set button as reset if true (overrides submit type).",
         "defaultValue": false}},
      {"submit": {
         "type": "BOOLEAN",
         "description": "Set button as submit if true (default). If false,",
         "defaultValue": false}},
      {"src": {
         "type": "STRING",
         "description": "Source for image (overrides submit and reset types).",
         "defaultValue": ""}},
      {"style": {
         "type": "STRING",
         "description": "Specify style rules inline.",
         "defaultValue": ""}},
      {"tabIndex": {
         "type": "INT",
         "description": "Position in tabbing order.",
         "defaultValue": -1}},
      {"title": {
         "type": "STRING",
         "description": "Provides a title for element.",
         "defaultValue": ""}},
      {"value": {
         "type": "STRING",
         "description": "Value of input.",
         "defaultValue": ""}},
      {"visible": {
         "type": "BOOLEAN",
         "description": "Hide or show element.",
         "defaultValue": false}}
    ],
    "config": {
      "type": {
        "id": "@JS_NAME@",
        "libs": [
            "@JS_PATH@/bootstrap.js",
            "@JS_PATH@/webui.js"
        ],
        "resources": [ "@JS_LIBPATH@" ]
        }
    }
}
